set lines=80 columns=85
set guifont=Inconsolata-dz\ 9
set guioptions-=T
set linespace=3

macmenu File.Close key=<nop>
macmenu Window.Select\ Next\ Tab key=<c-right>
macmenu Window.Select\ Previous\ Tab key=<c-left>
macmenu Edit.Font.Bigger key=<d-+>

macmenu File.Open\.\.\. key=<nop>

nnoremap ' cs"'<cr>

